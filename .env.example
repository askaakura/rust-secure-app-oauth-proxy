OAUTH2_PROXY_PROVIDER=google # authentication provider (google, github, gitlab, etc.)
OAUTH2_PROXY_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
OAUTH2_PROXY_CLIENT_SECRET=your_google_client_secret

# generate a strong random secret (e.g., openssl rand -base64 32 or python3 -c 'import os,base64; print(base64.urlsafe_b64encode(os.urandom(32)).decode())' )
OAUTH2_PROXY_COOKIE_SECRET=generate_a_strong_random_secret_here

# leave empty to enforce whitelist.txt only, "*" for all domains, or specify domain
OAUTH2_PROXY_EMAIL_DOMAINS=yourdomain.com

OAUTH2_PROXY_HTTP_ADDRESS=0.0.0.0:4180
OAUTH2_PROXY_HTTPS_ADDRESS=:443
OAUTH2_PROXY_REVERSE_PROXY=true
OAUTH2_PROXY_SET_XAUTHREQUEST=true
OAUTH2_PROXY_COOKIE_SECURE=true
OAUTH2_PROXY_COOKIE_DOMAIN=yourdomain.com
OAUTH2_PROXY_COOKIE_NAME=_oauth2_proxy # optional
OAUTH2_PROXY_COOKIE_REFRESH=1h # optional

OAUTH2_PROXY_REDIRECT_URL=https://yourdomain.com/oauth2/callback
OAUTH2_PROXY_AUTHENTICATED_EMAILS_FILE=/whitelist.txt

OAUTH2_PROXY_STANDARD_LOGGING=true
OAUTH2_PROXY_REQUEST_LOGGING=true
